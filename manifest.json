{
    "manifest_version": 2,
    "name": "NetClave Security Extension",
    "description": "This component is part of NetClave - an identity based network access solution. More information can be found here - https://www.blackvisor.io/products/",
    "version": "1.0",
    "background": {
        "scripts": ["js/storage.js", "js/crypto-utils.js", "js/crypto-storage.js", "js/axios.min.js", "js/session-storage.js", "js/background.js"],
        "persistent": true
    },
    "permissions": [
        "cookies",
        "*://*/",
        "notifications"
    ],
    "browser_action": {
        "default_icon": {
          "16": "images/blackvisor_16.png",
          "32": "images/blackvisor_32.png",
          "64": "images/blackvisor_64.png",
          "128": "images/blackvisor_128.png"
        }
    },
    "icons": {
        "16": "images/blackvisor_16.png",
        "32": "images/blackvisor_32.png",
        "64": "images/blackvisor_64.png",
        "128": "images/blackvisor_128.png"
    }
}